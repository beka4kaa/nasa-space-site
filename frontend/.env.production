# Frontend Production Environment
# NASA KOI Portal Client Settings

# API Configuration - Railway Backend URL
NEXT_PUBLIC_API_URL=https://nasa-space-site-production.up.railway.app
NEXT_PUBLIC_API_VERSION=v1

# App Configuration
NEXT_PUBLIC_APP_NAME=NASA KOI Portal
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_APP_DESCRIPTION=NASA Kepler Objects of Interest Analysis Portal

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_EXPORT=true
NEXT_PUBLIC_ENABLE_DARK_MODE=true

# File Upload Limits
NEXT_PUBLIC_MAX_FILE_SIZE=104857600
NEXT_PUBLIC_ALLOWED_EXTENSIONS=csv,xls,xlsx

# Performance Settings
NEXT_PUBLIC_PREDICTION_TIMEOUT=120000
NEXT_PUBLIC_API_TIMEOUT=30000

# Debug Settings
NEXT_PUBLIC_DEBUG=false
NEXT_PUBLIC_LOG_LEVEL=error